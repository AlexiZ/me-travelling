{% extends "form_div_layout.html.twig" %}

{% block form_label %}
    {% spaceless %}
        {% import _self as macros %}

        {% if label is empty %}
            {% set label = macros.form_parent_name(form) %}
        {% else %}
            {% set label = label|trans %}
        {% endif %}

        {{ parent() }}
    {% endspaceless %}
{% endblock form_label %}